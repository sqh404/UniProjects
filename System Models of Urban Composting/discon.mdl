{UTF-8}
Accumulation Discrete= INTEG (
	pulse to 100 for 6 hours at time 10-outflow discrete,
		0)
	~	Gallon
	~		~	:SUPPLEMENTARY 
	|

outflow discrete=
	DELAY FIXED(pulse to 100 for 6 hours at time 10,processing time,0)
	~	Gallon/Hour
	~		|

Accumulation 1= INTEG (
	pulse to 100 for 6 hours at time 10-outflow 1,
		0)
	~	Gallon
	~		|

Accumulation 3 Step 1= INTEG (
	pulse to 100 for 6 hours at time 10-step 1 to 2,
		0)
	~	Gallon
	~		|

Accumulation 3 Step 2= INTEG (
	step 1 to 2-step 2 to 3,
		0)
	~	Gallon
	~		|

Accumulation 3 Step 3= INTEG (
	+step 2 to 3-outflow 3,
		0)
	~	Gallon
	~		|

Accumulation N= INTEG (
	pulse to 100 for 6 hours at time 10-outflow n,
		0)
	~	Gallon
	~		~	:SUPPLEMENTARY 
	|

n=
	12
	~	Dmnl [1,100,1]
	~		|

outflow 1=
	Accumulation 1/processing time
	~	Gallon/Hour
	~		|

outflow 3=
	Accumulation 3 Step 3/stage time
	~	Gallon/Hour
	~		|

outflow n=
	DELAY N(pulse to 100 for 6 hours at time 10, processing time, 0, n)
	~	Gallon/Hour
	~		|

processing time=
	24
	~	Hour
	~		|

pulse to 100 for 6 hours at time 10=
	100*PULSE(10,6)
	~	Gallon/Hour
	~		|

stage time=
	processing time/3
	~	Hour
	~		|

step 1 to 2=
	Accumulation 3 Step 1/stage time
	~	Gallon/Hour
	~		|

step 2 to 3=
	Accumulation 3 Step 2/stage time
	~	Gallon/Hour
	~		|

